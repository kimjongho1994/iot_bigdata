#include <stdio.h>
/* 躍 AND 쵖쟕킨
 * A촜썒 B촜썒 C촜썒 써쎀
 * 썐  썐  썐  썐
 * 츂    썐  썐  썐
 * 츂    츂    썐  썐
 * 츂    츂     츂    츂
 * 썐  츂    썐  썐 
 * 츂    썐   츂   썐
 * 썐  츂     츂   썐
 * 썐  썐   츂   썐
*/
void main(){
	int num1=10;
	int num2=12;
	int num3=14;
	int result;

	printf("num1: %d, num2: %d\n", num1, num2);

    // 썐 && 썐 && 썐
	result = (num1 == 1 && num2 == 1 num3 == 1);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
    
	// 츂 && 썐 && 썐
	result = (num1 == 10 && num2 == 1 num3 == 1);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
	
	// 츂 && 츂 && 썐
	result = (num1 == 10 && num2 == 12 num3 == 1);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
	
	// 츂 && 츂 && 츂
	result = (num1 == 10 && num2 == 12 num3 == 14);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
	
	// 썐 && 츂 && 썐
	result = (num1 == 1 && num2 == 12 num3 == 1);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
	
	// 츂 && 썐 && 츂
	result = (num1 == 10 && num2 == 1 num3 == 14);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
	
	// 썐 && 츂 && 츂
	result = (num1 == 1 && num2 == 12 num3 == 14);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
	
	// 썐 && 썐 && 츂
	result = (num1 == 1 && num2 == 1 num3 == 14);
	printf("(num1 ==10 && num2==12 && num3 == 14) => 銓써쎀: %d\n", result);
}
